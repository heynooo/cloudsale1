<?xml version="1.0" encoding="utf-8"?>
<LinearLayout style="@style/base_activity"
              xmlns:android="http://schemas.android.com/apk/res/android"
              android:background="#f0f0f0"
              android:clipChildren="false"
    >


    <!-- 标题栏 -->
    <include layout="@layout/base_ativity_head"/>
    <!--标签-->


        <android.support.v4.view.ViewPager
            android:layout_marginTop="20px"
            android:id="@+id/home_pager_viewpager"
            android:layout_width="655px"
            android:layout_height="442px"
            android:layout_gravity="center"
            android:layout_margin="15px"
            android:clipChildren="false"
            ></android.support.v4.view.ViewPager>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:orientation="vertical"
            android:weightSum="1">


            <EditText
                android:id="@+id/roomName"
                style="@style/editText_update_set"
                android:layout_weight="3"
                android:hint="自定义房间名称"
                />
        </LinearLayout>



</LinearLayout>