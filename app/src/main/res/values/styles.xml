    </style>

    <style name="seekbar">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginRight">80px</item>
        <item name="android:layout_weight">330</item>
        <item name="android:max">100</item>
        <item name="android:maxHeight">12px</item>
        <item name="android:minHeight">12px</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:progressDrawable">@drawable/po_seekbar</item>
        <item name="android:thumb">@mipmap/img_seekbar_boll</item>
        <item name="android:thumbOffset">10dp</item>
    </style>

    <style name="scener_parting">
        <item name="android:layout_width">match_parent</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@mipmap/pop_scener_parting</item>
        <item name="android:layout_height">1px</item>
